{
  "expo": {
    "name": "nexaspay",
    "slug": "nexaspay",
    "scheme": "nexaspay",
    "version": "1.0.0",
    "extra": {
      "router": {
        "origin": false
      }
    },
    "web": {
      "favicon": "./assets/icons/nexas-ia.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font",
      "expo-barcode-scanner"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "icon": "./assets/logo/nexaspay_logo.png",
    "splash": {
      "image": "./assets/logo/nexaspay_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  }
}
